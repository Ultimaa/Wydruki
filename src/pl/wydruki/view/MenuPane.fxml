<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" 
fx:controller="pl.wydruki.controller.MenuPaneController">
   <children>
      <MenuBar fx:id="aa" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu fx:id="bbb" mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Open" />
                  <MenuItem fx:id="close" mnemonicParsing="false" text="Zamknij" />
            </items>
          </Menu>
          <Menu fx:id="ccc" mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem fx:id="option" mnemonicParsing="false" text="Ustawienia" />
            </items>
          </Menu>
          <Menu fx:id="ddd" mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
